{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Yarp": "Warning",
      "LibraHub": "Debug"
    }
  },
  "ConnectionStrings": {
    "Redis": "localhost:6379"
  },
  "Services": {
    "Identity": "http://localhost:60951",
    "Catalog": "http://localhost:60961",
    "Content": "http://localhost:59533",
    "Orders": "http://localhost:55918",
    "Library": "http://localhost:60991",
    "Notifications": "http://localhost:61001"
  },
  "ReverseProxy": {
    "Routes": {
      "notifications-signalr-hub": {
        "ClusterId": "notifications",
        "Match": {
          "Path": "/hubs/notifications/{**catch-all}"
        },
        "AuthorizationPolicy": "RequireAuthenticated",
        "Transforms": [
          {
            "PathPattern": "/hubs/notifications/{**catch-all}"
          }
        ]
      }
    },
    "Clusters": {
      "identity": {
        "Destinations": {
          "default": {
            "Address": "http://localhost:60951"
          }
        }
      },
      "catalog": {
        "Destinations": {
          "default": {
            "Address": "http://localhost:60961"
          }
        }
      },
      "content": {
        "Destinations": {
          "default": {
            "Address": "http://localhost:59533"
          }
        }
      },
      "orders": {
        "Destinations": {
          "default": {
            "Address": "http://localhost:55918"
          }
        }
      },
      "library": {
        "Destinations": {
          "default": {
            "Address": "http://localhost:60991"
          }
        }
      },
      "notifications": {
        "Destinations": {
          "default": {
            "Address": "http://localhost:61001"
          }
        }
      }
    }
  }
}
